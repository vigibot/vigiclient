{
 "SECUREMOTEPORT": 8042,
 "VIDEOLOCALPORT": 8043,
 "AUDIOLOCALPORT": 8044,

 "LOGFILE": "/var/log/vigiclient.log",

 "WIFIFILE": "/proc/net/wireless",
 "TEMPFILE": "/sys/class/thermal/thermal_zone0/temp",
 "WIFIRATE": 250,
 "TEMPRATE": 1000,
 "CPURATE": 250,

 "PROCESSDIFFUSION": "/usr/local/vigiclient/processdiffusion",
 "PROCESSDIFFVIDEO": "/usr/local/vigiclient/processdiffvideo",
 "PROCESSDIFFVIDEO": "/usr/local/vigiclient/processdiffcustm",
 "PROCESSDIFFAUDIO": "/usr/local/vigiclient/processdiffaudio",

 "CMDDIFFUSION": [
  [
   "/usr/local/vigiclient/processdiffusion",
   " /dev/video0",
   " | /bin/nc 127.0.0.1 VIDEOLOCALPORT",
   " -w 2"
  ], [
   "/usr/local/vigiclient/processdiffvideo",
   " -loglevel fatal",
   " -f fbdev",
   " -r FPS",
   " -i /dev/fb0",
   " -c:v h264_omx",
   " -profile:v baseline",
   " -b:v BITRATE",
   " -flags:v +global_header",
   " -bsf:v dump_extra",
   " -f rawvideo",
   " tcp://127.0.0.1:VIDEOLOCALPORT"
  ], [
   "/usr/local/vigiclient/processdiffcustm",
   " WIDTH HEIGHT FPS",
   " | /usr/bin/ffmpeg",
   " -loglevel fatal",
   " -s:v WIDTHxHEIGHT",
   " -pix_fmt rgb24",
   " -f rawvideo",
   " -i -",
   " -c:v h264_omx",
   " -profile:v baseline",
   " -b:v BITRATE",
   " -flags:v +global_header",
   " -bsf:v dump_extra",
   " -f rawvideo",
   " tcp://127.0.0.1:VIDEOLOCALPORT"
  ]
 ],

 "CMDDIFFAUDIO": [
  "/usr/local/vigiclient/processdiffaudio",
  " -loglevel fatal",
  " -f alsa",
  " -ac 1",
  " -i hw:RECORDINGDEVICE,0",
  " -ar 16000",
  " -c:a pcm_s16le",
  " -f s16le",
  " tcp://127.0.0.1:AUDIOLOCALPORT"
 ],

 "CMDTTS": "/usr/bin/espeak -v fr -f /tmp/tts.txt --stdout | /usr/bin/aplay -D plughw:PLAYBACKDEVICE",

 "UPTIMEOUT": 5000,
 "V4L2": "/usr/bin/v4l2-ctl",
 "LATENCYALARMEND": 250,
 "LATENCYALARMBEGIN": 500,
 "EMERGENCYBITRATE": 50000,
 "TXRATE": 50,
 "SERVORATE": 20,
 "BEACONRATE": 10000,

 "CW2015ADDRESS": 98,
 "MAX17043ADDRESS": 54,
 "BQ27441ADDRESS": 85,
 "GAUGERATE": 250,

 "PCA9685FREQUENCY": 50,

 "INPUT": 2,
 "UNUSED": -1
}
